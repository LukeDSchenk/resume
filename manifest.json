{
  "manifest_version": 3,
  "name": "Resume",
  "version": "1.0.0",
  "description": "Show a text list of all URLs in the current window, in tab order.",
  "action": {
    "default_title": "Resume",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "48": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "resume@maraud.earth",
      "strict_min_version": "58.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
